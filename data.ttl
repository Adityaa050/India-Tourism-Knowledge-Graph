@prefix it: <http://example.org/india-tourism#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

# --- 1. Ontology (Rules) ---

# Classes
it:TouristAttraction rdf:type rdfs:Class .
it:City rdf:type rdfs:Class .
it:State rdf:type rdfs:Class .
it:AttractionType rdf:type rdfs:Class .
it:Restaurant rdf:type rdfs:Class .

# Properties
it:locatedIn rdf:type rdf:Property ;
    rdfs:label "Located In" ;
    rdfs:range it:City .
it:cityInState rdf:type rdf:Property ;
    rdfs:label "City in State" ;
    rdfs:domain it:City ;
    rdfs:range it:State .
it:hasType rdf:type rdf:Property ;
    rdfs:label "Has Type" ;
    rdfs:domain it:TouristAttraction ;
    rdfs:range it:AttractionType .
it:bestTimeToVisit rdf:type rdf:Property ;
    rdfs:label "Best Time To Visit" .
it:servesCuisine rdf:type rdf:Property ;
    rdfs:label "Serves Cuisine" ;
    rdfs:domain it:Restaurant .

# --- 2. Attraction Types (Instances) ---
it:Monument rdf:type it:AttractionType .
it:Beach rdf:type it:AttractionType .
it:Fort rdf:type it:AttractionType .
it:Temple rdf:type it:AttractionType .
it:Lake rdf:type it:AttractionType .
it:Palace rdf:type it:AttractionType .
it:NationalPark rdf:type it:AttractionType .
it:Garden rdf:type it:AttractionType .
it:HillStation rdf:type it:AttractionType .
it:Ashram rdf:type it:AttractionType .

# --- 3. Data (Facts) ---

# --- States & UTs ---
it:UP rdf:type it:State ; rdfs:label "Uttar Pradesh" .
it:Rajasthan rdf:type it:State ; rdfs:label "Rajasthan" .
